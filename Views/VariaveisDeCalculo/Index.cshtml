            <th>
                @Html.DisplayNameFor(model => model.Icms)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FreteVenda)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FreteCompra)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ProducaoMensal)
            </th>
            <th></th>
        </tr>
